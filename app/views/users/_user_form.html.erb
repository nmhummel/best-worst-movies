
<%= form_with model: @user do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %><br>
    <br>
    <%= f.label :email %>
    <%= f.email_field :email %><br>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %><br>
    <br>
    <%= f.label :best_worst_movie %>
    <%= f.text_field :best_worst_movie %><br>
    <br>
    <%= f.label :avatar %>
    <%= f.file_field :avatar %><br>
    <br>
    <%= f.submit %>
<% end %>