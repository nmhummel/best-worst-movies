<h1>All Movies</h1>

<table>
 <tr>
     <td>
     <% @movies.each do |m| %>
        <%= m.poster%>
     </td>
     <td>
        <%= m.title %><br>
        <%= m.genre %><br>
        <%= m.rating %><br>
        <%= m.year %><br>
        <%= link_to "Rank #{m.title}", new_movie_ranking_path(m.id) %>     
        <% end %>
     </td>
  </tr>

</table>